{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "../web",
    "distDir": "../web",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "Finanzbuch",
    "version": "0.1.7"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": ["deb", "appimage"],
      "identifier": "com.robertosw.finanzbuch",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ], 
      "category": "Finance"
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Finanzbuch",
        "width": 1920,
        "height": 820
      }
    ]
  }
}
